// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Item de um card√°pio
model Item {
  id           String @id @default(cuid())
  nome         String
  preco        Float
  descricao_pt String
  descricao_en String
  categoria    String
}
